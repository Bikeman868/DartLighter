{
    "name": "dart-language",
    "displayName": "Dart Language",
    "description": "Provides syntax highlighting for the Dart programming language",
    "version": "0.0.1",
    "publisher": "bikeman868",
    "engines": {
        "vscode": "^1.14.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "dart",
            "aliases": ["Dart", "dart"],
            "extensions": [".dart"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "dart",
            "scopeName": "source.dart",
            "path": "./syntaxes/dart.tmLanguage.json"
        }]
    }
}